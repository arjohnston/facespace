    "ignore-styles": "^5.0.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.10.2",
    "standard": "^14.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "proxy": "http://localhost:8080/",
  "jest": {
    "testURL": "http://localhost/"
  },
  "standard": {
    "globals": [
      "fetch",
      "$"
    ],
    "parser": "babel-eslint"
